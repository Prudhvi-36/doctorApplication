<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.doctordashboard">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DoctorDashboard">
        <activity android:name=".UpdateProfilePatient"></activity>
        <activity android:name=".Reschedule" />
        <activity android:name=".ActiveAppointments" />
        <activity android:name=".Payment" />
        <activity android:name=".BookAppointment" />
        <activity android:name=".DoctorList" />
        <activity android:name=".PatientDashboard" />
        <activity android:name=".Specialist" />
        <activity android:name=".DoctorSchedule" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UpdateProfile" />
    </application>

</manifest>